{
  "item": [
    {
      "id": "aa442edf-8efa-484e-8c73-b5e512a00039",
      "name": "Successful lookup with status available",
      "request": {
        "url": {
          "path": [
            "pet",
            "findByStatus"
          ],
          "host": [
            "{{baseUrl}}"
          ],
          "query": [
            {
              "key": "status",
              "value": "available"
            }
          ],
          "variable": []
        },
        "header": [
          {
            "key": "accept",
            "value": "application/json"
          }
        ],
        "method": "GET"
      },
      "response": [],
      "event": [
        {
          "listen": "test",
          "script": {
            "id": "a4c09516-f5d3-49c5-9cfc-37fa5728f13e",
            "type": "text/javascript",
            "exec": [
              "const jsonData = (() => {",
              "  try {",
              "    return pm.response.json();",
              "  } catch (e) {",
              "    return null;",
              "  }",
              "})();",
              "function resolvePath(obj, path) {",
              "  if (!obj || typeof path !== 'string') { return undefined; }",
              "  const segments = path.replace(/\\[(\\d+)\\]/g, '.$1').split('.').filter(Boolean);",
              "  return segments.reduce((acc, key) => { return acc && acc[key] !== undefined ? acc[key] : undefined; }, obj);",
              "}",
              "pm.test(\"Assertion 1: status\", function () {",
              "  pm.response.to.have.status(200);",
              "});",
              "pm.test(\"Assertion 2: header_present\", function () {",
              "  pm.expect(pm.response.headers.has(\"Content-Type\")).to.eql(true);",
              "});",
              "pm.test(\"Assertion 3: content_type_includes\", function () {",
              "  pm.expect(pm.response.headers.get('Content-Type')).to.include(\"application/json\");",
              "});",
              "pm.test(\"Assertion 4: json_is_array\", function () {",
              "  pm.expect(Array.isArray(jsonData)).to.eql(true);",
              "});",
              "pm.test(\"Assertion 5: json_array_min_length\", function () {",
              "  const length = Array.isArray(jsonData) ? jsonData.length : 0;",
              "  pm.expect(length).to.be.at.least(0);",
              "});",
              "pm.test(\"Assertion 6: json_path_exists\", function () {",
              "  pm.expect(resolvePath(jsonData, \"[0].id\") !== undefined).to.eql(true);",
              "});"
            ]
          }
        }
      ]
    },
