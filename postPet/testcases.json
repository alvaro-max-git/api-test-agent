{
  "name": "POST /pet - Add Pet",
  "baseUrl": "{{baseUrl}}",
  "defaultHeaders": {
    "accept": "application/json",
    "Content-Type": "application/json"
  },
  "variables": {
    "baseUrl": "https://petstore.swagger.io/v2"
  },
  "endpoint": {
    "method": "POST",
    "path": "/pet"
  },
  "testcases": [
    {
      "id": "TC-001",
      "name": "Successfully add a new pet with a valid status: available",
      "method": "POST",
      "path": "/pet",
      "query": {},
      "headers": {
        "accept": "application/json",
        "Content-Type": "application/json"
      },
      "body": {
        "id": 0,
        "category": {
          "id": 0,
          "name": "string"
        },
        "name": "doggie",
        "photoUrls": [
          "string"
        ],
        "tags": [
          {
            "id": 0,
            "name": "string"
          }
        ],
        "status": "available"
      },
      "requiresAuth": false,
      "expectedStatus": 200,
      "assertions": [
        { "type": "status", "equals": 200 },
        { "type": "header_present", "name": "Content-Type" },
        { "type": "content_type_includes", "value": "application/json" },
        { "type": "json_path_exists", "path": "id" },
        { "type": "json_path_exists", "path": "name" },
        { "type": "json_path_exists", "path": "photoUrls[0]" },
        { "type": "json_path_exists", "path": "status" }
      ]
    },
